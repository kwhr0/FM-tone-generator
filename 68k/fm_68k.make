#   File:       fm_68k.make#   Target:     fm_68k#   Created:    Thursday, July 3, 2014 07:22:29 AMMAKEFILE        = fm_68k.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :obj:Includes        = Sym-68K         = -sym offCPlusOptions    = {Includes} {Sym-68K} -model far -mc68020 -opt speed -load prefix.pch### Source Files ###SrcFiles        =  				  Channel8.cpp 				  ChannelManager.cpp 				  FMMain.cpp 				  fm_68k.r 				  main.cpp 				  Midi.cpp 				  Operator8.cpp 				  table.r 				  tone.r### Object Files ###ObjFiles-68K    =  				  "{ObjDir}Channel8.cpp.o" 				  "{ObjDir}ChannelManager.cpp.o" 				  "{ObjDir}FMMain.cpp.o" 				  "{ObjDir}main.cpp.o" 				  "{ObjDir}Midi.cpp.o" 				  "{ObjDir}Operator8.cpp.o"### Libraries ###LibFiles-68K    =  				  "{CLibraries}CLib881.o" 				  "{Libraries}MathLib881.o" 				  "{CLibraries}StdCLib.o" 				  "{CLibraries}IOStreams881.o" 				  "{CLibraries}CPlusLib.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.o" 				  "{Libraries}ToolLibs.o" 				  "{Libraries}Interface.o"### Default Rules ###.cpp.o    .cpp  {쩗ondoBuild}	{CPlus} {depDir}{default}.cpp -o {targDir}{default}.cpp.o {CPlusOptions}### Build Rules ###fm_68k  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	ILink 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'APPL' 		-c 'FM68' 		-model far 		-state rewrite 		-compact -pad 0	If "{Sym-68K}" =~ /-sym [nNuU]/		ILinkToSYM {Targ}.NJ -mf -sym 3.2 -c 'sade'	Endfm_68k  컴  fm_68k.r {쩗ondoBuild}	Rez fm_68k.r -o {Targ} {Includes} -appendfm_68k  컴  table.r {쩗ondoBuild}	Rez table.r -o {Targ} {Includes} -appendfm_68k  컴  tone.r {쩗ondoBuild}	Rez tone.r -o {Targ} {Includes} -append### Required Dependencies ###"{ObjDir}Channel8.cpp.o"    Channel8.cpp"{ObjDir}ChannelManager.cpp.o"    ChannelManager.cpp"{ObjDir}FMMain.cpp.o"    FMMain.cpp"{ObjDir}main.cpp.o"    main.cpp"{ObjDir}Midi.cpp.o"    Midi.cpp"{ObjDir}Operator8.cpp.o"    Operator8.cpp### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-ignore "{RIncludes}" 		-objdir "{ObjDir}" 		-objext .o 		{Includes} 		{SrcFiles}